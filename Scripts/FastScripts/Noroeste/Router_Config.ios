!--Configuracion de Router Noroeste [Subinterfaces VLAN'ss & RIP]
en
conf t
hostname Noroeste

int g0/0.10
enc dot1q 10
ip addr 130.45.43.1 255.255.255.192

int g0/0.15
enc dot1q 15
ip addr 130.45.43.65 255.255.255.192

int g0/0.20
enc dot1q 20
ip addr 130.45.43.129 255.255.255.192

int g0/0.90
enc dot1q 90
ip addr 130.45.43.193 255.255.255.192

int g0/0
no shut

int s0/0/0
